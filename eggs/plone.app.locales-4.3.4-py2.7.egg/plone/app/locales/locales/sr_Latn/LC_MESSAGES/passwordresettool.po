#: PasswordResetTool/skins/PasswordReset/mail_password_template.pt:27
#, fuzzy
msgid "mailtemplate_text_expirationdate_linkreset"
msgstr "Ovaj zahtev za promenu lozinke ističe za ovoliko sati: ${hours}, datuma: ${expirationdate}."

#. Default: "The following link will take you to a page where you can reset your password for ${site_name} site: ${reset_url}"
#: PasswordResetTool/skins/PasswordReset/mail_password_template.pt:20
#, fuzzy
msgid "mailtemplate_text_linkreset"
msgstr "Neko je tražio da lozinka za Vaš nalog za „${site_name}‟ bude promenjena. Ukoliko to niste bili Vi, možete slobodno da zanemarite ovo obaveštenje. Ako želite da nastavite i promenite lozinku,  molimo da posetite adresu: ${reset_url}"

#. Default: "If you didn't expect to receive this email, please ignore it. Your password has not been changed. Request made from IP address ${ipaddress}"
#: PasswordResetTool/skins/PasswordReset/mail_password_template.pt:36
#, fuzzy
msgid "mailtemplate_tracking_information"
msgstr "Podaci o praćenju:  zahtev je poslat sa IP adrese ${ipaddress}.  Vreme: ${datetime_now}"

